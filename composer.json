{
    "name": "edipoelwes/laravel-mysql-spatial",
    "description": "MySQL spatial data types extension for Laravel.",
    "scripts": {
        "test": "phpunit -c phpunit.xml.dist",
        "test:unit": "phpunit -c phpunit.xml.dist --testsuite unit",
        "test:integration": "phpunit -c phpunit.xml.dist --testsuite integration"
    },
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Edipo Elwes",
            "email": "edipoelwes2@gmail.com"
        }
    ],
    "require": {
        "php": "^8.0",
        "ext-pdo": "*",
        "ext-json": "*",
        "illuminate/database": "^8.0|^9.0|^10.0|^11.0",
        "geo-io/wkb-parser": "^1.0",
        "jmikola/geojson": "^1.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5.10|^10.0|^11.0",
        "laravel/laravel": "^8.0|^9.0|^10.0|^11.0",
        "doctrine/dbal": "^3.3|^4.0",
        "laravel/browser-kit-testing": "^6.3",
        "mockery/mockery": "^1.4.4|^1.5|^1.6"
    },
    "autoload": {
        "psr-4": {
            "EdipoElwes\\LaravelMysqlSpatial\\": "src/"
        }
    },
    "autoload-dev" : {
        "classmap" : [
            "tests/Unit",
            "tests/Integration"
        ]
    },
    "extra": {
        "branch-alias": {
            "dev-master": "4.0.x-dev"
        },
        "laravel": {
            "providers": [
                "EdipoElwes\\LaravelMysqlSpatial\\SpatialServiceProvider"
            ]
        }
    }
}
